{
    "manifest_version": 2,
    "name": "Amazon sold filter",
    "version": "1.0",
  
    "description": "Shows only lines that include selling stats",
  
    "icons": {
      "48": "icons/border-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.amazon.co.uk/*"],
        "js": ["main.js"]
      }
    ]
  }
  